<wxs module="filter" src="../../../utils/money.wxs"></wxs>
<wxs module="tools">
    var signInfoShow = function(name,sign_info_name) { 
    var res=false         
    if (sign_info_name == name)
        res = true;
    if (sign_info_name != undefined && sign_info_name.indexOf(name) != -1)
        res = true;
    return res;
    }
    module.exports = {
        signInfoShow: signInfoShow,
    }
</wxs>
<view class="m-20">
    <van-field
        value="{{ sign_comment }}"
        label="审批意见"
        type="textarea"
        placeholder="请输入审批意见"
        autosize
        border="{{ false }}"
        show-word-limit="{{true}}"
        maxlength="200"
        autosize="{ maxHeight: 250, minHeight: 150 }"
        bind:input="onSignComment"
    />
    <van-row gutter="10" custom-class="mt-20">
    <van-col span="12"><van-button color="#198fff"  block size="small"  bind:tap="goSignBill" data-item="approve">同意</van-button></van-col>
    <van-col span="12"><van-button color="#999" block size="small"  bind:tap="goSignBill" data-item="reject">拒绝</van-button></van-col>
    </van-row>
</view>
<view class="work-list  container">
    <view class="title font-28 c-198fff  line2 bor-b">签核信息</view>
    <view wx:for="{{signUserList}}" wx:key="index" class="step-list bg-fff font-24 c-999 container" >
        <van-row >
            <van-col span="16" offset="2" custom-class="c-333">
                <span class="c-333 d-block"> {{item.emp_name}} </span>                
            </van-col>
            <van-col span="6"  custom-class="text-right"><text class="c-198fff">{{item.status_desc}}</text></van-col>
            <van-col span="22" offset="2" custom-class="font-20">{{filter.formatTime(item.sign_date)}}</van-col>
        </van-row>
        <view class="circle-container"><text class="step-circle {{item.status=='A'?'active':''}}">{{item.sign_order_no}}</text></view>
        <view class="step-line {{item.status=='A'?'active':''}}"></view>
    </view>  
</view>
